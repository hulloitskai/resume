{
  "name": "vue-resume",
  "description": "A resume written in Vue.",
  "version": "0.1.0",
  "author": "Steven Xie <hello@stevenxie.me>",
  "repository": "https://github.com/stevenxie/vue-resume",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "gh-pages -d dist/ -b static",
    "produce": "yarn build && yarn deploy",
    "deploy-gh": "gh-pages -d dist/",
    "produce-gh": "GH_PAGES=true yarn build && yarn deploy-gh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{vue,js,css,scss}": [
      "yarn lint",
      "git add"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "vue": "^2.5.21"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.2.0",
    "@vue/cli-plugin-eslint": "^3.2.1",
    "@vue/cli-service": "^3.1.3",
    "@vue/eslint-config-prettier": "^4.0.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.1.3",
    "lint-staged": "^8.0.4",
    "node-sass": "^4.10.0",
    "sass-loader": "^7.1.0",
    "vue-template-compiler": "^2.5.21"
  }
}
